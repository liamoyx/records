{
    "title": "记录",
    "description": "记录平时使用的软件等",
    "author": "Xintao",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "ignores": [
        "node_modules",
        "book.json"
    ],
    "links": {
        "sidebar": {
            "Home": "https://xinntao.github.io/"
        }
    },
    "plugins": [
        "-lunr",
        "search",
        "katex",
        "simple-page-toc",
        "anchor-navigation-ex",
        "copy-code-button",
        "expandable-chapters"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "anchor-navigation-ex": {
            "showLevel": false,
            "associatedWithSummary": true,
            "printLog": false,
            "multipleH1": true,
            "mode": "float",
            "showGoTop": true,
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right"
        }
    }
}